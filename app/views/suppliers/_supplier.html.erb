<tr id=<%= "supplier_#{supplier.id}" %>>
<td><%= link_to supplier.id, edit_supplier_path(supplier), :remote => true %></td>
<td><%= supplier.document %></td>
<td><%= supplier.social_reason %></td>
<td><%= supplier.phone_one %></td>
<td><%= supplier.phone_two %></td>
<td><%= supplier.address %></td>
<td><%= supplier.email %></td>
<td><%= supplier.state %></td>
<td>
<%= link_to image_tag('shows.png', :title  => "Mostrar Proveedor"), supplier, :remote => true, :class => 'btn btn-mini' %>
<%= link_to image_tag('editar.png', :title  => "Editar Proveedor"), edit_supplier_path(supplier), :remote => true,
:class => 'btn btn-mini' %>
<%= link_to image_tag('deletes.png', :title  => "Eliminar Proveedor"), supplier,
:data => { :confirm => 'Esta Seguro?' },
:method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
</td>
</tr>
