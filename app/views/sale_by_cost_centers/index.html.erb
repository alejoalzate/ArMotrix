<%- model_class = SaleByCostCenter -%>
<div class="page-header">
  <h1><u>Venta por Centro de Costo</u></h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>#</th>
      <th>Desde</th>
      <th>Hasta</th>
      <th>Centro De Costos</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <%= render @sale_by_cost_centers %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to image_tag('new.png', :title  => "Nueva venta"), new_sale_by_cost_center_path, :remote => true %></td>
    </tr>
  </tfoot>
</table>
