<%- model_class = Bank -%>
<div class="page-header">
<h1>Banco</h1>
</div>
<div class='bank'>
<p><b>Numero</b>: <%= @bank.id %></p>
<p><b>Nombre</b>: <%= @bank.name %></p>
<p><b>Siglas</b>: <%= @bank.acronym %></p>
<p><b>Dirección</b>: <%= @bank.address %></p>
<p><b>Telefono</b>: <%= @bank.phone %></p>
<p><b>Descripción</b>: <%= @bank.description %></p>
</div>
